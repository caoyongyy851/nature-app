<view class="px-2"><block wx:for="{{placeList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="mb-5"><view data-event-opts="{{[['tap',[['toPlaceDetal',['$0'],[[['placeList','',index]]]]]]]}}" bindtap="__e"><view class="swiperContainer mb-2"><swiper style="width:100%;height:500rpx;" autoplay="{{false}}" duration="{{1000}}"><block wx:for="{{item.videoList}}" wx:for-item="item1" wx:for-index="index1" wx:key="index1"><block wx:if="{{item.videoList.length>0}}"><swiper-item><video class="rounded-top-1" style="width:100%;height:500rpx;" src="{{getImgBase+item1}}" autoplay="true" muted="{{true}}" loop="true" object-fit="fill" show-play-btn="{{false}}" show-fullscreen-btn="{{false}}"></video></swiper-item></block></block><block wx:for="{{item.imageList}}" wx:for-item="item1" wx:for-index="index1" wx:key="index1"><swiper-item><image class="rounded" style="width:100%;height:500rpx;" src="{{getImgBase+item1}}" mode="aspectFill" lazy-load="true"></image></swiper-item></block></swiper><view class="imageCount flex align-center justify-center">{{(item.videoList?item.videoList.length:0+item.imageList.length)+'图'}}</view></view></view><view class="pt-2"><text class="font-md text-muted">{{item.name}}</text></view><view class="pt-2 flex align-center justify-between"><view class="flex align-center"><block wx:for="{{item.labelList}}" wx:for-item="item1" wx:for-index="index1" wx:key="index1"><view class=" font-sm  text-secondary mr-2 py-1 px-2 rounded-1" style="background-color:rgba(0, 0, 0, 0.05);">{{''+item1+''}}</view></block></view><view class="flex align-center" style="background-color:#f0fcff;"><view class=" px-2 py-1 rounded"><u-icon vue-id="{{'56f60eff-1-'+index}}" name="attach" color="#00c6ff" size="30" bind:__l="__l"></u-icon><text style="color:#00c6ff;">{{item.bugs+"人预定"}}</text></view></view></view></view></block></view>