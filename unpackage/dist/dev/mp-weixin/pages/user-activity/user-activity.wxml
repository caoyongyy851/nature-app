<view><block wx:if="{{activityList.length>0}}"><u-toast class="vue-ref" vue-id="cc6194c8-1" data-ref="uToast" bind:__l="__l"></u-toast><block wx:for="{{activityList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><u-swipe-action vue-id="{{'cc6194c8-2-'+index}}" show="{{item.show}}" index="{{index}}" options="{{options}}" data-event-opts="{{[['^click',[['click']]],['^open',[['open']]],['^contentClick',[['contentClick']]]]}}" bind:click="__e" bind:open="__e" bind:contentClick="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="m-2 flex"><view><image class="rounded" style="height:150rpx;width:150rpx;" mode="aspectFill" src="{{getImgBase+item.img}}"></image></view><view class="flex-column flex-1 ml-2"><view class="font-md"><text>{{item.title}}</text></view><view class="mt-1 font text-muted " style="overflow:hidden;text-overflow:ellipsis;display:-webkit-box;-webkit-line-clamp:3;-webkit-box-orient:vertical;"><text>{{item.detail}}</text></view><block wx:if="{{item.activityStatus==1}}"><view class="mt-1 font flex justify-between"><text class="text-muted">{{"人数："+item.perLimit+"/"+item.person}}</text><text class="text-main">进行中</text></view></block><block wx:else><view class="mt-1 font flex justify-between"><text class="text-muted">{{"人数："+item.perLimit+"/"+item.person}}</text><text class="text-muted">已结束</text></view></block></view></view></u-swipe-action></block></block><block wx:else><view class="flex align-center justify-center" style="margin-top:400rpx;"><u-empty vue-id="cc6194c8-3" text="数据为空" mode="list" bind:__l="__l"></u-empty></view></block></view>